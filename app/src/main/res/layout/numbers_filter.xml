<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:weightSum="3">
    <ImageView
        android:id="@+id/close_filter_numbers"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.7"
        android:background="@color/black"
        android:alpha="0.3"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.3"
        android:background="@color/white"
        android:paddingStart="@dimen/_15sdp"
        android:paddingEnd="@dimen/_15sdp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/toolbar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:paddingTop="@dimen/_20sdp">
            <TextView
                android:id="@+id/filter_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/number_close_btn"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/reset_numbers_btn"
                app:layout_constraintBottom_toBottomOf="@id/number_close_btn"
                android:text="@string/filters"
                android:textAllCaps="true"
                android:fontFamily="@font/objectivity_medium"
                android:textColor="@color/blue_gray"
                android:textSize="@dimen/_10ssp"/>
            <TextView
                android:id="@+id/number_close_btn"
                android:layout_width="@dimen/_25sdp"
                android:layout_height="@dimen/_25sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@drawable/circular_gray_border"
                android:text="X"
                android:textColor="@color/blue_gray"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_10ssp"/>
            <TextView
                android:id="@+id/reset_numbers_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toStartOf="@id/number_close_btn"
                app:layout_constraintStart_toEndOf="@id/filter_title"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:background="@drawable/circular_orange_border"
                android:text="@string/reset"
                android:textColor="@color/base_orange"
                android:textAllCaps="true"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_10ssp"
                android:paddingStart="@dimen/_10sdp"
                android:paddingEnd="@dimen/_10sdp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <ScrollView
            android:id="@+id/plate_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/numbers_filter_results"
            android:layout_marginBottom="@dimen/_10sdp"
            android:visibility="gone">
            <LinearLayout
                android:id="@+id/plate_numbers_layout"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/category_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/numbers"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <TextView
                    android:id="@+id/category"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/category_title"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/plate_number"
                    android:background="@drawable/gray_border_raduis_5"
                    android:padding="@dimen/_5sdp"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/blue_gray"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/emirate"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_border_raduis_5"
                android:orientation="vertical">
                <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                    android:id="@+id/emirate_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_8sdp"/>

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/plate_type"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_border_raduis_5"
                android:orientation="vertical">
                <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                    android:id="@+id/plate_type_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_8sdp"/>
            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/plate_code"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_border_raduis_5"
                android:orientation="vertical">
                <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                    android:id="@+id/plate_code_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_8sdp"/>
            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/digits"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RadioButton
                        android:id="@+id/digit1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:checked="true"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_1_digit"/>
                    <RadioButton
                        android:id="@+id/digit2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_2_digit"/>
                    <RadioButton
                        android:id="@+id/digit3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_3_digit"/>
                    <RadioButton
                        android:id="@+id/digit4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_4_digit"/>
                    <RadioButton
                        android:id="@+id/digit5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_5_digit"/>
                </RadioGroup>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/_10sdp">
                    <TextView
                        android:id="@+id/price_range_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/used_cars_trim_spinner"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="@string/price_range"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:textSize="@dimen/_9ssp"/>
                    <TextView
                        android:id="@+id/aud_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/price_range_title"
                        app:layout_constraintStart_toEndOf="@id/price_range_title"
                        android:text="@string/in_aed"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/gray"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:textSize="@dimen/_9ssp"/>
                </LinearLayout>
                <com.rizlee.rangeseekbar.RangeSeekBar
                    android:id="@+id/plate_price_seek_bar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/price_range_title"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:active="true"
                    app:minValue="0"
                    app:maxValue="10000000"
                    app:activeColor="@color/base_violet"
                    app:centerColor="@color/base_violet"
                    app:defaultColor="@color/base_violet"
                    app:sideColor="@color/gray"
                    app:additionalTextMargin="6dp"
                    app:roundedCorners="true"
                    app:showThumbsText="above"
                    app:textColor="@android:color/black"
                    app:thumbsTextMargin="6dp" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/location_title12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="@string/location"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:textSize="@dimen/_9ssp"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/location_RV12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/location_title12"
                        app:layout_constraintStart_toStartOf="parent" />
                    <ImageView
                        android:id="@+id/add_location12"
                        android:layout_width="@dimen/_25sdp"
                        android:layout_height="@dimen/_25sdp"
                        app:layout_constraintTop_toTopOf="@id/location_title12"
                        app:layout_constraintStart_toEndOf="@id/location_title12"
                        app:layout_constraintBottom_toBottomOf="@id/location_title12"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:src="@drawable/add_location"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </ScrollView>

        <ScrollView
            android:id="@+id/mobile_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/numbers_filter_results"
            android:layout_marginBottom="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:visibility="visible">
            <LinearLayout
                android:id="@+id/mobile_numbers_layout"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/numbers"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <TextView
                    android:id="@+id/category_mobile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/category_title"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/mobile_number"
                    android:background="@drawable/gray_border_raduis_5"
                    android:padding="@dimen/_8sdp"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/blue_gray"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/operator"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/gray_border_raduis_5"
                    android:orientation="vertical">
                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/operators_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_8sdp"/>

                </androidx.appcompat.widget.LinearLayoutCompat>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/code"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/gray_border_raduis_5"
                    android:orientation="vertical">
                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/codes_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_8sdp"/>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mobile_plan"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/gray_border_raduis_5"
                    android:orientation="vertical">
                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/mobile_plan_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_8sdp"/>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/digits"
                    android:textAllCaps="true"
                    android:fontFamily="@font/objectivity_medium"
                    android:textColor="@color/purple_text"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="@dimen/_9ssp"/>
                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RadioButton
                        android:id="@+id/digit3_similar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:checked="true"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_3_digit_similar"/>
                    <RadioButton
                        android:id="@+id/digit4_similar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_4_digit_similar"/>
                    <RadioButton
                        android:id="@+id/digit5_similar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_5_digit_similar"/>
                    <RadioButton
                        android:id="@+id/digit6_similar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_6_digit_similar"/>
                    <RadioButton
                        android:id="@+id/digit7_similar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_9ssp"
                        android:text="@string/_7_digit_similar"/>
                </RadioGroup>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/_10sdp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/used_cars_trim_spinner"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="@string/price_range"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:textSize="@dimen/_9ssp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/price_range_title"
                        app:layout_constraintStart_toEndOf="@id/price_range_title"
                        android:text="@string/in_aed"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/gray"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:textSize="@dimen/_9ssp"/>
                </LinearLayout>
                <com.rizlee.rangeseekbar.RangeSeekBar
                    android:id="@+id/mobile_price_seek_bar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/price_range_title"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:active="true"
                    app:minValue="0"
                    app:maxValue="10000000"
                    app:activeColor="@color/base_violet"
                    app:centerColor="@color/base_violet"
                    app:defaultColor="@color/base_violet"
                    app:sideColor="@color/gray"
                    app:additionalTextMargin="6dp"
                    app:roundedCorners="true"
                    app:showThumbsText="above"
                    app:textColor="@android:color/black"
                    app:thumbsTextMargin="6dp" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/location_title7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="@string/location"
                        android:textAllCaps="true"
                        android:fontFamily="@font/objectivity_medium"
                        android:textColor="@color/purple_text"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:textSize="@dimen/_9ssp"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/location_RV7"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/location_title7"
                        app:layout_constraintStart_toStartOf="parent" />
                    <ImageView
                        android:id="@+id/add_location7"
                        android:layout_width="@dimen/_25sdp"
                        android:layout_height="@dimen/_25sdp"
                        app:layout_constraintTop_toTopOf="@id/location_title7"
                        app:layout_constraintStart_toEndOf="@id/location_title7"
                        app:layout_constraintBottom_toBottomOf="@id/location_title7"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:src="@drawable/add_location"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </ScrollView>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/numbers_filter_results"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:cornerRadius="@dimen/_15sdp"
            android:backgroundTint="@color/base_orange"
            android:layout_marginBottom="@dimen/_10sdp"
            android:text="@string/filter_results"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:gravity="center"
            android:fontFamily="@font/objectivity_medium"
            android:textSize="@dimen/_11ssp"
            android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>